    {
      "name": "LinkCard",
      "type": "a",
      "props": {
        "color": "yellow",
        "hover": {
          "color": "#fff",
          "& img": {
            "transition": "all 0.10s",
            "filter": "hue-rotate(177deg)",
            "outline-style": "solid",
            "outline-width": "4px",
            "outline-color": "white"
          }
        }
      },
      "style": {
        "webkitTextDecoration": "none",
        "textDecoration": "none"
      },
      "examples": [
        "<LinkCard>\nHello</LinkCard>"
      ],
      "system": [
        "hover",
        "active",
        "focus"
      ]
    },
    {
      "name": "ImageYellow",
      "type": "Image",
      "props": {
        "bg": "yellow"
      },
      "style": {
        "backgroundBlendMode": "multiply"
      },
      "examples": [
        "<ImageYellow src='https://images.unsplash.com/photo-1484728863103-3f0403e03a7b?auto=format&fit=crop&w=3302&q=80&ixid=dW5zcGxhc2guY29tOzs7Ozs%3D' />"
