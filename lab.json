        "LinkCard",
        "ImageYellow"
      ],
      "jsx": "<Box bg='black' width='100%'>\n  <LinkCard href={props.href}>\n    <ImageYellow src={props.src}>\n    </ImageYellow>\n    <Heading mt={2}>\n      {props.title}\n    </Heading>\n  </LinkCard>\n</Box>",
      "examples": [
        "<VideoCard bg='black' \nsrc='https://images.unsplash.com/photo-1484728863103-3f0403e03a7b?auto=format&fit=crop&w=3302&q=80&ixid=dW5zcGxhc2guY29tOzs7Ozs%3D'\nhref='#url'\ntitle='hello'\n/>"
      ],
      "system": []
